/*/////////////////////////////////

	GLOBAL STYLES 

/////////////////////////////////*/

body { 
	font-size: 1.25em;
	font-family: 'Open Sans', sans-serif;
	background: #FFF;
}

h1, h2, h3, h4, h5, h6 {
	font-family: 'Open Sans', sans-serif;
	font-weight: 100;
	color: #58585b;
}

p, a, ul, li {
	font-family: 'Open Sans', sans-serif;
	font-weight: 300;
	color: #58585b;
}

a {
	text-decoration: none;
	font-size: 1em;
	color: #ef4856; 
}

a:hover {
	opacity: 0.6;
}


h1 {
	font-size: 3em;
	color: #58585b;
	line-height: 2em;
	margin: 25px 0px 25px 0px;
}

h2 {
	font-size: 2em;
	line-height: 1.5em;
	font-weight: 200;
	color: #58585b;
}

h3 {
 	font-size: 1.5em;
 	font-weight: 200;
 	line-height: 1.5em;
 	color: #58585b;
}


p {
	margin-bottom: 20px;
	font-size: 1em;
	font-weight: 300;
	line-height: 1.5em;
	color: #58585b;
}

ul {
	margin-bottom: 20px;
}


ul li {
	font-size: 1em;
	font-weight: 200;
	color: #595959;
	line-height: 1.75em;
	margin-left: 40px;
}


hr {
	margin: 50px 0px 50px 0px;
	border: 1px solid #f1f1f1;
}

.container {
	width: 100%;
	margin: 0 auto;
	padding: 0;
}

.body-container {
	width: 100%; 
	margin: 0 auto;
	padding: 0px;
}



/*/////////////////////////////////
 
 	MAIN MENU 

/////////////////////////////////*/

.menu-area {
	width: 100%;
   	height: 80px;
   	background-color:#FFF;
